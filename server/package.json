    "cors": "^2.8.5",
    "debug": "~2.6.0",
    "diskspace": "*",
    "express": "~4.14.1",
    "express-session": "^1.17.1",
    "js-htmlencode": "0.3.0",
    "jsonwebtoken": "^7.4.1",
    "mocha": "^8.2.1",
    "moment": "2.8.4",
    "moment-timezone": "*",
    "mongoose": "^4.8.5",
    "morgan": "~1.7.0",
    "mysql": "*",
    "nodejs-websocket": "^1.7.2",
    "pg": "^8.5.1",
    "pug": "*",
    "q": "*",
    "redis": "^3.0.2",
    "sendgrid": "~5.0.0",
