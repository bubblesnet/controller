{
  "plugins": [
    "plugins/markdown",
    "plugins/summarize",
    "better-docs/component"
  ],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "better-docs": {
      "name": "BubblesNet React components"
    }
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "../README.md"
  }
}